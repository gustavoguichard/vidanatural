{
  "public": true,
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://vidanatural.eco.br" }
    }
  ],
  "build": {
    "env": {
      "API_IP": "@api-ip",
      "API_DOMAIN": "@api-domain",
      "API_PROXY": "@api-proxy",
      "ANALYTICS_ID": "@analytics-uid",
      "GTM_ID": "@gtm-id"
    }
  }
}
