  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "yarn tw --minify && next build",
    "dev": "next dev",
    "lint:eslint": "eslint ./",
    "lint:prettier": "prettier --check \"{**/*.js,**/*.jsx,**/*.ts,**/*.tsx}\"",
    "lint:types": "tsc --project tsconfig.json --noEmit",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:types",
    "postbuild": "ts-node ./scripts/generate-sitemap.js && ts-node ./scripts/generate-rss.js",
    "start": "next start",
    "test:ci": "yarn test:unit",
    "test:unit": "jest",
    "test": "yarn test:unit --watch",
